player[] owned_bots: CountOf(FilteredArray(AllyPlayers(), Curr().owner == player && IsDummyBot(Curr()) && EntityExists(Curr())));           // amount of owners bots
player[] human_cnt: CountOf(FilteredArray(AllyPlayers(), !IsDummyBot(Curr())));                                                             // amount of all human players on a team
player[] spawned_human_cnt: CountOf(FilteredArray(AllyPlayers(), !IsDummyBot(Curr()) && HasSpawned(Curr())));                               // amount of all human players on a team that have spawned
rule: "[Owner] :: Create Bots"
Event.OngoingPlayer
if (!IsDummyBot())
if (HasSpawned())
if (IsAlive())
if (owned_bots < Floor((TEAM_SIZE - spawned_human_cnt) / human_cnt))
{
    bot_slot = FirstOf(FilteredArray(slot_arr, 
                    !EntityExists(PlayersInSlot(Curr(), TeamOf())) || 
                    (IsDummyBot(Curr()) && !EntityExists(Curr().owner)))); // sets the slot for the bot to be created in if there is no entity present and no dummy bot exists at the end of the slot_arr. returns a number

    vec position = (PositionOf() + Vector(0, 0.2, 0));
    CreateDummyBot(HeroOf(), TeamOf(), bot_slot, RayCastHitPosition(position, position + WorldVectorOf(Vector(0, 0, -1.5), player)), FacingDirectionOf()); // create the bot

    PlayersInSlot(bot_slot, TeamOf()).owner = player; // assign the owner

    MinWait();

    LoopIfConditionIsTrue();
}

rule: "[Owner] :: Destroy Bots"
Event.OngoingPlayer
if (!IsDummyBot())
if (owned_bots > Floor((TEAM_SIZE - human_cnt) / human_cnt))
{
    DestroyDummyBot(TeamOf(), 
                    LastOf(FilteredArray(slot_arr, 
                    IsDummyBot(PlayersInSlot(Curr(), TeamOf())) && PlayersInSlot(Curr(), TeamOf()).owner == player && EntityExists(PlayersInSlot(Curr(), TeamOf())))));

    MinWait();

    LoopIfConditionIsTrue();
}


